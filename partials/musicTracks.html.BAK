<div class="container-fluid">
  <div class="row-fluid">
    <div class="span12">
      <h1>MY MUSIC</h1>
          <p>The Latest Music I've Uploaded</p>
    </div>
  </div>
</div>

<div class="container-fluid">
  <div class="row-fluid">

    <div class="span3">
		<!--Sidebar content-->
      <p>Search: <input ng-model="query"></p>
      <p>Sort by:
      <select ng-model="orderProp">
        <option value="name">Alphabetical</option>
        <option value="age">Elevation Gain</option>
        <option value="age">Distance</option>
        <option value="age">Highest Peak</option>
      </select></p>
    </div>

    <div class="span7 offset2">
      <!--Main content-->
      <p>Visuals of The Latest Music I've Uploaded</p>
      <ul class="musicTracks">
        <li ng-repeat="track in musicTracks | filter:query | orderBy:orderProp"
            class="thumbnail track-listing">
          <a href="#/musicTracks/{{track.id}}" class="thumb"><img ng-src="{{track.imageUrl}}"></a>
          <a href="#/musicTracks/{{track.id}}">{{track.name}}</a>
          <p>{{track.snippet}}</p>
        </li>
      </ul>
    </div>

  </div>
</div>
